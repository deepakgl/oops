
<!-- About Company detail section -->
<section id="company-detail">
  <div class="container">
  <div class="row">
      <div class="col-md-6 company-detail-description wow fadeInRight">
        <h2>{{content.field_title.0}}</h2>
        <p>{{content.field_description.0}}</p>
      </div>
      <div class="col-md-6 company-detail-img wow fadeInLeft">
        <img src="{{ file_url(content.field_company_image['#items'].entity.uri.value)}}"
             class="img-fluid" 
             alt="{{ content.field_company_image['#items'].alt }}" 
             title="{{ content.field_company_image['#items'].title }}">
      </div>
    </div>
  </div>
</section>
<!-- End About Company detail section -->
